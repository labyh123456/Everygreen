<h2>Register Customer Management </h2>

<table border="1" cellpadding="5">
    <tr>
      <th>index</th>  
    <th>_id</th>
    <th>username</th>
    <th>Email</th>
    <th>Action</th>
    </tr>
    <% let counter = 0 %>
<% users.forEach(user => { %>
    
    <tr>
        <td> <%= ++counter %></td>
        <td><%= user._id %> </td>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td>   <%- include("../../partials/delete_form", {url:`/admin/register_customer_management/delete/${user._id}` }) %></td>
    </tr>

<% }) %>    
</table>