<h2>header</h2>
<form action="/everygreen/submit_review" method="POST">
<h1>Your Review   : <%= customer.email %></h1>
<hr>
<h2><%= book.title %></h2>
<img src="<%= book.coverImagePath %>" height="110px" width="84px">
<div class="container">
    <div id="rateYo"></div>
    <input type="hidden" id="rating" name="rating">
    <input type="hidden" id="bookid" name="bookid"  value="<%= book._id %>">
    <input type="text" name="headline">
    <textarea name="comment" id="" cols="30" rows="10"></textarea>
    <button type="submit">Save</button>
    <a href="/">cancel</a>
</div>
</form>
